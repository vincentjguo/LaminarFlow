{
  "manifest_version": 3,
  "name": "LaminarFlow",
  "description": "Enhance UWFlow user experience",
  "background": {},
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "icon-128.png", "icon-34.png"],
      "matches": []
    }
  ],
  "optional_host_permissions": ["*://*/"],
  "permissions": [
    "storage",
    "cookies"
  ]
}
